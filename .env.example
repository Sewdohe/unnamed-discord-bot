DISCORD_TOKEN=your_discord_bot_token
CLIENT_ID=your_client_id
GUILD_ID=your_guild_id_for_development

# MongoDB connection string (without database name)
# For MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net
# For local MongoDB: mongodb://localhost:27017
MONGODB_URI=mongodb://localhost:27017

# MongoDB database name (defaults to "navi-bot")
# Change this if running multiple bots on the same MongoDB server
MONGODB_DATABASE=discord-bot
